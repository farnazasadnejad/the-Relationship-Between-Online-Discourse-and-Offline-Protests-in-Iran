word_counts_per_hour = defaultdict(int)

iran_timezone = timezone(timedelta(hours=3, minutes=30))

for post_time, post_content in data:
    post_time_iran = post_time.astimezone(iran_timezone)
    hour_key = post_time_iran.replace(minute=0, second=0, microsecond=0)

    word_count = len(re.findall(r'\bشریف\b', post_content, re.IGNORECASE))

    word_counts_per_hour[hour_key] += word_count

hours, word_counts = zip(*sorted(word_counts_per_hour.items()))

fig, ax = plt.subplots()

ax.plot(hours, word_counts, marker='o')

ax.xaxis.set_major_formatter(DateFormatter("%Y-%m-%d %H:%M:%S"))


plt.xticks(rotation=45, ha="right")

ax.set_xlabel('Time (Date and Hour in Iran\'s timezone)')
ax.set_ylabel('Word Count of "شریف"')
ax.set_title('Word Count of "شریف" Per Hour')


plt.tight_layout()
plt.show()
